\begin_inset Formula 
\begin{align*}
\begin{array}{c|cc}
+ & 0 & 1\\[0.1in]
\hline 0 & 0 & 1\\
1 & 1 & 0
\end{array} &  & \begin{array}{c|cc}
\gmult & 0 & 1\\[0.1in]
\hline 0 & 0 & 0\\
1 & 0 & 1
