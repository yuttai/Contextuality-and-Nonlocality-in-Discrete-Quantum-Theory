  FormBox[
   RowBox[{
    RowBox[{"\[ForAll]", 
     RowBox[{"A", "\[Element]", "\[ScriptCapitalA]"}]}], ",", 
    RowBox[{
     RowBox[{
      SubscriptBox["i", "m"], "(", "A", ")"}], "=", 
     RowBox[{"[", 
