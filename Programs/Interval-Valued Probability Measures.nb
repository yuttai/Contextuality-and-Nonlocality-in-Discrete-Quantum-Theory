      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"v", "[", "#", "]"}], "\[Equal]", 
         RowBox[{"1", "-", 
          RowBox[{"Min", "[", 
           RowBox[{"i", "[", 
            RowBox[{"Complement", "[", 
             RowBox[{"S", ",", "#"}], "]"}], "]"}], "]"}]}]}], "&"}], "/@", 
