 &  & 1=f(0,0,1,-1)+f(1,-1,0,0)+f(1,1,1,1)+f(1,1,-1,-1)\textrm{ ,}\\
 &  & 1=f(1,0,1,0)+f(0,1,0,1)+f(1,1,-1,-1)+f(1,-1,-1,1)\textrm{ .}\label{eq:Kernaghan-2}
\end{eqnarray}

\end_inset

In equation
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand ref
reference "eq:Kernaghan-1"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:Kernaghan-2"

